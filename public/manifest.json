{
  "name": "Restaurant App",
  "short_name": "Restaurant",
  "description": "A restaurant ordering app.",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/icons/icon-384x384.png",
      "sizes": "384x384",
      "type": "image/png"
    }
  ],
  "serviceworker": {
    "src": "/service-worker.js",
    "type": "module"
  },
  "scope": "/",
  "orientation": "portrait",
  "display": "standalone",
  "prefer_related_applications": false,
  "shortcuts": [
    {
      "name": "View Menu",
      "short_name": "Menu",
      "description": "Quick access to the restaurant menu.",
      "url": "/menu",
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Order Online",
      "short_name": "Order",
      "description": "Place an order for delivery or takeout.",
      "url": "/order",
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ]
}
